{
    "name": "iip-design/cdp-video-post-type",
    "type": "wordpress-plugin",
    "description" : "CDP Video Post-Type enables you to create video content. The plugin is not publicly accessible, so a page url will not be generated for this content. It serves only to submit video content to be index into the CDP.",
    "authors": [
        {
            "name": "Office of Design, U.S. Department of State",
            "email": "orelusm@america.gov"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "git",
            "url": "git@github.com:JamesHeinrich/getID3.git"
        }
    ],
    "minimum-stability": "stable",
    "require": {
       "composer/installers": "~1.0",
       "wpackagist-plugin/cmb2": "2.3.0",
       "james-heinrich/getid3": "v1.9.15"
    },
    "autoload"    : {
        "files": [
            "vendor/cmb2/cmb2/init.php",
            "vendor/james-heinrich/getid3/getid3/getid3.php"
        ]
    },
    "extra": {
        "installer-paths": {
            "vendor/cmb2/cmb2": ["wpackagist-plugin/cmb2"],
            "vendor/james-heinrich/getid3": ["james-heinrich/getid3"]
        }
    }
}